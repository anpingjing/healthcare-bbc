# Docker Compose 环境变量示例
# 复制此文件为 .env 并填入实际值

# MySQL 配置
MYSQL_ROOT_PASSWORD=YourSecureRootPassword123!
MYSQL_USER=healthcare
MYSQL_PASSWORD=YourSecureDbPassword123!

# Redis 配置
REDIS_PASSWORD=YourSecureRedisPassword123!

# JWT 密钥（生产环境请生成随机密钥）
JWT_SECRET=CHANGE_ME

# 企业微信配置
WECHAT_CORP_ID=
WECHAT_CORP_SECRET=
WECHAT_AGENT_ID=
WECHAT_TOKEN=
WECHAT_ENCODING_AES_KEY=

ENCRYPTION_KEY=0123456789abcdef0123456789abcdef

APP_CORS_ALLOWED_ORIGIN_PATTERNS=http://localhost:5173,http://localhost:3000

# 其他配置
SPRING_PROFILES_ACTIVE=prod
